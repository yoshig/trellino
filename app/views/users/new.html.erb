<h1>New User</h1>

<form action="<%= users_url %>" method="POST">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

	<label for="email">Email</label>
	<input id="email" type="text" name="user[email]">


	<label for="password">Password</label>
	<input id="password" type="password" name="user[password]">

	<input id="logout" type='submit' value='Login'>
</form>

<a href="<%= new_session_url %>">Have An Account?</a>