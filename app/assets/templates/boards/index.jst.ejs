<% boards.forEach(function(board) { %>
  <li>
    <a href="#/boards/<%= board.escape("id") %>">
      <%= board.escape("title") %>
    </a>

    <a href="#/boards/<%= board.escape("id") %>/edit">
      Edit
    </a>

    <button class="destroy-board" data-id="<%= board.escape("id") %>">
      Destroy
    </button>
  </li>
<% }) %>

<a href="#/boards/new">New Board</a>